{% extends "front/base.html.twig" %}
   
{% block content %}
   <section >
       <article class="container">
            <div class="row">
                <div class="col-xs-12"><h2>Enregistrement</h2></div>
            </div>

            <div class="container">
                {{ form_start(form) }}
                {{ form_row(form.userPseudo) }}
                {{ form_row(form.email) }}
                {{ form_row(form.plainPassword.first) }}
                {{ form_row(form.plainPassword.second) }}

                <button type="submit" class="btn btn-default">S'enregistrer</button>
                {{ form_end(form) }}
            </div>
       </article>
    </section>

{% endblock %}




